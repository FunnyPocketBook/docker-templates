<?xml version="1.0"?>
<Container version="2">
  <Name>firefly-iii-bunq-importer</Name>
  <Repository>fireflyiii/bunq-importer</Repository>
  <Registry>https://hub.docker.com/r/fireflyiii/bunq-importer/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/96221-support-funnypocketbook-firefly-iii</Support>
  <Project>https://github.com/firefly-iii/bunq-importer</Project>
  <Overview>This is a tool to import from bunq into Firefly III. It works by using your bunq API token and a Firefly III personal access token to access your Firefly III installation's API.</Overview>
  <Category>Productivity:</Category>
  <WebUI>http://[IP]:[PORT:8081]/</WebUI>
  <TemplateURL/>
  <Icon>https://camo.githubusercontent.com/794353350b564a5d0c0244cd6d82586c9ffd22f4/68747470733a2f2f7777772e66697265666c792d6969692e6f72672f7374617469632f696d672f6c6f676f2d736d616c6c2d6e65772e706e67</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1598666768</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>This is a tool to import from bunq into Firefly III. It works by using your bunq API token and a Firefly III personal access token to access your Firefly III installation's API.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8081</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment>
    <Variable>
      <Value/>
      <Name>FIREFLY_III_ACCESS_TOKEN</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>http://192.168.0.10:8080</Value>
      <Name>FIREFLY_III_URI</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>BUNQ_API_CODE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>BUNQ_API_URI</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="FIREFLY_III_ACCESS_TOKEN" Target="FIREFLY_III_ACCESS_TOKEN" Default="" Mode="" Description="Container Variable: FIREFLY_III_ACCESS_TOKEN - Personal Access Token" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="FIREFLY_III_URI" Target="FIREFLY_III_URI" Default="" Mode="" Description="Container Variable: FIREFLY_III_URI - URI of Firefly-III instance" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="BUNQ_API_CODE" Target="BUNQ_API_CODE" Default="" Mode="" Description="Container Variable: BUNQ_API_CODE" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="BUNQ_API_URI" Target="BUNQ_API_URI" Default="" Mode="" Description="Container Variable: BUNQ_API_URI" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="Port" Target="80" Default="" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always" Required="false" Mask="false">8081</Config>
  <Config Name="APP_KEY" Target="APP_KEY" Default="" Mode="" Description="Container Variable: APP_KEY" Type="Variable" Display="always" Required="false" Mask="false">SomeRandomStringOf32CharsExactly</Config>
</Container>
