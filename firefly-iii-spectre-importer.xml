<?xml version="1.0"?>
<Container version="2">
  <Name>firefly-iii-spectre-importer</Name>
  <Repository>fireflyiii/spectre-importer</Repository>
  <Registry>https://hub.docker.com/r/fireflyiii/spectre-importer/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/96221-support-funnypocketbook-firefly-iii</Support>
  <Project>https://github.com/firefly-iii/spectre-importer</Project>
  <Overview>A tool to import from Salt Edge / Spectre into Firefly III.</Overview>
  <Category>Productivity:</Category>
  <WebUI>http://[IP]:[PORT:8080]/</WebUI>
  <TemplateURL/>
  <Icon>https://camo.githubusercontent.com/794353350b564a5d0c0244cd6d82586c9ffd22f4/68747470733a2f2f7777772e66697265666c792d6969692e6f72672f7374617469632f696d672f6c6f676f2d736d616c6c2d6e65772e706e67</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1598665839</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>A tool to import from Salt Edge / Spectre into Firefly III.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8081</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment>
    <Variable>
      <Value/>
      <Name>FIREFLY_III_ACCESS_TOKEN</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>http://192.168.0.10:8080</Value>
      <Name>FIREFLY_III_URL</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>SPECTRE_APP_ID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>SPECTRE_SECRET</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Firefly-III Personal Access Token" Target="FIREFLY_III_ACCESS_TOKEN" Default="" Mode="" Description="Container Variable: FIREFLY_III_ACCESS_TOKEN - Personal Access Token" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="Firefly-III URL" Target="FIREFLY_III_URL" Default="" Mode="" Description="Container Variable: FIREFLY_III_URL" Type="Variable" Display="always" Required="true" Mask="false">http://192.168.0.10:8080</Config>
  <Config Name="SPECTRE_APP_ID" Target="SPECTRE_APP_ID" Default="" Mode="" Description="Container Variable: SPECTRE_APP_ID" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="SPECTRE_SECRET" Target="SPECTRE_SECRET" Default="" Mode="" Description="Container Variable: SPECTRE_SECRET" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="Port" Target="8080" Default="" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always" Required="true" Mask="false">8081</Config>
</Container>
